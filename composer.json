{
    "name": "cornford/bencoded",
    "description": "An easy way to use bencoding in Laravel.",
    "keywords": ["laravel", "bencode", "bencoding", "encode", "decode"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bradley Cornford",
            "email": "me@bradleycornford.co.uk"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.0",
        "mockery/mockery": "^1.2.4",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-0": {
            "Cornford\\Bencoded": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Cornford\\Bencoded\\BencodedServiceProvider"
            ],
            "aliases": {
                "Bencoded": "Cornford\\Bencoded\\Facades\\BencodedFacade"
            }
        }
    }
}
